{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "webextensions": true
  },
  "extends": [
    "standard",
    "standard-react"
  ],
  "rules": {
    "comma-dangle": [
      "warn",
      "never"
    ],
    "indent": "warn",
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "key-spacing": "off",
    "no-console": "warn",
    "no-debugger": "error",
    "no-irregular-whitespace": "off",
    "object-curly-spacing": [
      2,
      "always"
    ],
    "react/prop-types": "warn",
    "space-before-function-paren": "off"
  }
}
